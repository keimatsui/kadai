/<h3>/{               #h3要素の行の
  gsub(/<[^>]*>/, "");#タグを消去
  gsub(/ \(.*\)/, "");#(Mon)などを消去
  date = $0;          #日付を取得
}

/<li[ |>]/{           #<li>要素の行の
  gsub(/<[^>]*>/, "");#タグを消去
  gsub(/\s{2,}/, ""); #2つ以上続く空白を消去
  gsub(/\(|\)/, "");  #丸括弧()を消去
  gsub(/,/, "");      #カンマを消去
  print date, $0;     #日付と合わせて表示
}
